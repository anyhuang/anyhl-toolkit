{
  "productName": "anyhl-toolkit",
  "version": "0.0.0",
  "identifier": "com.tauri.test",
  "build": {
    "beforeDevCommand": "pnpm run dev:ui",
    "beforeBuildCommand": "pnpm run build:ui",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../ui/dist"
  },
  "app": {
    "windows": [
      {
        "title": "黄某人的作战实验室",
        "width": 1920,
        "height": 1080
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
